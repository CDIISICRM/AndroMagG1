<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crm.commentmag"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="15" android:targetSdkVersion="15" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.crm.magazines.MagazineListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.crm.magazines.NewMagazineActivity"
            android:label="@string/NewMagazineActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.crm.commentaires.MagazineListActivity"/>
        </activity>
        <activity
            android:name="com.crm.commentaires.CommentaireListActivity"
            android:label="@string/CommentaireListActivity" >
        </activity>
        <activity
            android:name="com.crm.commentaires.NewCommentaireActivity"
            android:label="@string/NewCommentaireActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.crm.commentaires.CommentaireListActivity"/>
        </activity>
    </application>
</manifest>
